{
    "name": "alt-three/logger",
    "description": "A Logger Wrapper For Laravel 5",
    "keywords": ["logger", "logging", "Logger", "Alt Three"],
    "license": "MIT",
    "authors": [
        {
            "name": "James Brooks",
            "email": "james@alt-three.com"
        },
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        },
        {
            "name": "Joseph Cohen",
            "email": "joe@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^1.1",
        "graham-campbell/testbench": "^4.0",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "AltThree\\Logger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AltThree\\Tests\\Logger\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "AltThree\\Logger\\LoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
